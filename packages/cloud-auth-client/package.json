{
  "name": "@splunkdev/cloud-auth-client",
  "version": "1.0.0-alpha.2",
  "description": "Library to help authenticating web applications in Splunk Cloud.",
  "author": "Splunk, Inc.",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "node ci/extract_version.js && babel src -d . --extensions \".ts\"",
    "check-types": "tsc",
    "clean": "rimraf src/**/*.js src/**/*.js.map src/**/*.d.ts lib *.js *.js.map *.d.ts .nyc_output *.log junit.xml && yarn run clean:coverage || true",
    "clean:coverage": "rm -rf coverage",
    "clean:modules": "rimraf node_modules || true",
    "commit": "yarn git-cz",
    "cover": "yarn run clean:coverage && jest --config ./jest.json --coverage",
    "lint": "eslint \"src/**/*.ts\" && yarn lint:tests",
    "lint:fix": "eslint \"src/**/*.ts\" --fix && yarn lint:fix:tests",
    "lint:tests": "eslint \"test/**/*.ts\"",
    "lint:fix:tests": "eslint \"test/**/*.ts\" --fix",
    "lint:ci": "yarn lint -- -f junit -o test-reports/lint-results.xml",
    "prepack": "yarn build",
    "start": "babel src -d . --watch",
    "test": "jest --config ./jest.json",
    "test:ci": "yarn test --ci --reporters=default --reporters=jest-junit",
    "test:watch": "yarn test --watch",
    "third-party-licenses:generate": "node ./ci/generate_3rdparty.js",
    "third-party-licenses": "sh ./ci/scripts/generate_third_party_credits.sh"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@splunk/babel-preset": "^2.0.0",
    "@splunk/eslint-config": "^2.0.0",
    "@splunk/webpack-configs": "^4.0.0",
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "eslint-plugin-unicorn": "^15.0.1",
    "inject-loader": "^4.0.1",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "markdownlint": "^0.15.0",
    "mocha": "^7.0.0",
    "npm-license-crawler": "^0.2.1",
    "rimraf": "^2.6.2",
    "sinon": "^8.0.4",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.2",
    "webpack": "^3.11.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.5",
    "q": "^1.5.1",
    "url-parse": "^1.4.7"
  },
  "engines": {
    "node": ">=6"
  },
  "jest-junit": {
    "output": "./test-reports/unit-results.xml"
  },
  "xjest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "../../ci/.cz-config.js"
    }
  }
}
