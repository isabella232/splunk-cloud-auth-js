{
  "name": "@splunkdev/cloud-node-auth",
  "version": "0.1.0-alpha.8",
  "author": "Splunk, Inc.",
  "license": "Apache-2.0",
  "description": "Splunk Cloud Node Authentication Management for JavaScript",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "/src/*.js",
    "/src/*.js.map",
    "/src/*.d.ts",
    "/src/**/*.js",
    "/src/**/*.js.map",
    "/src/**/*.d.ts"
  ],
  "scripts": {
    "commit": "yarn git-cz",
    "lint": "tslint -p tsconfig.json -c tslint.json && yarn lint:tests",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix && yarn lint:fix:tests",
    "lint:tests": "tslint -p tsconfig.test.json -c tslint.json",
    "lint:fix:tests": "tslint -p tsconfig.test.json -c tslint.json --fix",
    "ts:docs": "rm -rf docs && yarn ts:docs:html && yarn ts:docs:md",
    "ts:docs:html": "typedoc --tsconfig tsconfig.json --readme none --mode file --out docs/html/ --inlineSourceMap --module commonjs --theme minimal --excludePrivate",
    "ts:docs:md": "typedoc --tsconfig tsconfig.json --readme none --mode file --out docs/md/ --inlineSourceMap --module commonjs --theme markdown --excludePrivate --mdHideSources --mdEngine github",
    "docs": "yarn ts:docs",
    "build": "node ci/extract_version.js && tsc -p tsconfig.json",
    "build:tests": "tsc -p tsconfig.test.json",
    "clean": "rimraf src/**/*.js src/**/*.js.map src/**/*.d.ts src/*.js src/*.js.map src/*.d.ts && yarn run clean:coverage || true",
    "clean:coverage": "rm -rf coverage",
    "cover": "yarn run clean:coverage && nyc npm run test",
    "pretest": "yarn lint && yarn build",
    "test": "yarn pretest && ts-mocha \"test/unit/**/*.ts\" -p tsconfig.test.json --exit --timeout=900000 --recursive",
    "test:examples": "yarn pretest && ts-mocha -p tsconfig.test.json --timeout=900000 \"test/examples/*.ts\"",
    "prepare": "npm run build",
    "publish:docs": "yarn ts:docs && npm add --no-save @splunk/cicd-tools --registry https://repo.splunk.com/artifactory/api/npm/npm && yarn cicd-publish-docs --force docs/md",
    "example": "./ci/integration/runExamples.sh",
    "third-party-licenses:generate": "node ./ci/generate_3rdparty.js",
    "third-party-licenses": "sh ./ci/scripts/generate_third_party_credits.sh"
  },
  "devDependencies": {
    "@splunkdev/cloud-sdk": "^4.0.0",
    "@splunkdev/splunk-cloud-auth-common": "^0.1.0-alpha.2",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.12.6",
    "@types/node-fetch": "^2.5.4",
    "@types/pem": "^1.9.5",
    "@types/sinon": "^7.5.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.2",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.11.0",
    "jsdoc": "^3.5.5",
    "markdownlint": "^0.14.1",
    "mocha": "^5.0.4",
    "npm-license-crawler": "^0.2.1",
    "nyc": "^14.1.1",
    "pem": "^1.14.2",
    "replace": "^1.1.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.5.0",
    "sleep-promise": "^8.0.1",
    "source-map-support": "^0.5.6",
    "standard-version": "^4.4.0",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.0",
    "tsc": "^1.20150623.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^1.2.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "nyc": {
    "all": true,
    "branches": 80,
    "lines": 90,
    "functions": 90,
    "statements": 90,
    "cache": false,
    "check-coverage": true,
    "exclude": [
      "src/index.ts",
      "src/version.ts",
      "src/**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".js"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.js"
    ],
    "instrument": true,
    "reporter": [
      "lcov",
      "text-summary",
      "text",
      "html"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "sourceMap": true
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "../../ci/.cz-config.js"
    }
  }
}
