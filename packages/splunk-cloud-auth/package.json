{
  "name": "@splunkdev/cloud-auth",
  "version": "1.1.0-alpha.5",
  "description": "Library to help authenticating web applications in Splunk Cloud.",
  "author": "Splunk, Inc.",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "babel src -d . --ignore \"**/tests/*\" --ignore \"**/src/tests/*\"",
    "commit": "yarn git-cz",
    "lint": "eslint src",
    "lint:ci": "yarn run lint -- -f junit -o test-reports/lint-results.xml",
    "prepack": "yarn build",
    "start": "babel src -d . --ignore \"**/tests/*.unit.js\" --watch",
    "test": "jest --config ./jest.json",
    "test:ci": "yarn run test --ci --reporters=default --reporters=jest-junit",
    "test:watch": "yarn run test --watch",
    "third-party-licenses:generate": "node ./ci/generate_3rdparty.js",
    "third-party-licenses": "sh ./ci/scripts/generate_third_party_credits.sh"
  },
  "devDependencies": {
    "@splunk/babel-preset": "^1.1.0",
    "@splunk/eslint-config": "^1.2.0",
    "@splunk/webpack-configs": "^2.0.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^15",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7.8.2",
    "inject-loader": "^3.0.0",
    "jest": "^23.6.0",
    "jest-junit": "^6.3.0",
    "markdownlint": "^0.15.0",
    "mocha": "^6.0.1",
    "npm-license-crawler": "^0.2.1",
    "sinon": "^3.0.0",
    "webpack": "^3.11.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.5",
    "q": "^1.5.1",
    "url-parse": "^1.4.7"
  },
  "engines": {
    "node": ">=6"
  },
  "jest-junit": {
    "output": "./test-reports/unit-results.xml"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "../../ci/.cz-config.js"
    }
  }
}
